#!/bin/sh
set -euf
top="$(git rev-parse --show-toplevel)"
"$top/bin/topics" |
while read topic; do
    dir="$top/topics/$topic/"
    if [ -e "$dir" ] && [ -f "$top/topics/$topic/index.md" ] && [ -f "$top/topics/$topic/tutorial-by-claude-opus-4-5.md" ]; then
        echo "$topic"
    fi
done
